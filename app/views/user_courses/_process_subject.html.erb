<ul class="timeline timeline-custom">
  <li class="timeline-inverted">
    <div class="timeline-panel">
      <div class="timeline-heading">
        <h4 class="timeline-title"></h4>
        <div class="timeline-body">
          <% UserSubject.statuses.each do |key,value| %>
            <% count = count_status_subject @user_course, key,
              @user_course.user.id %>
            <% subject = name_subject key %>
            <p class="btn btn-<%= type_status key %> btn-circle" >
              <p><%= subject %> <%= count %>/<%= @count_all_subject %></p>
            </p>
          <% end %>
        </div>
      </div>
    </div>
  </li>
</ul>
